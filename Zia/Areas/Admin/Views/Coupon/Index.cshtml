@model IEnumerable<Zia.Models.Coupon>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h1>Index</h1>

<p>
    <partial name="_btnCreateNewPartial" />
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Type)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MinimumAmount)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Dicount)
            </th>
            

            <th>
                @Html.DisplayNameFor(model => model.IsActive)
            </th>

            <th></th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Type)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MinimumAmount)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Dicount)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsActive)
            </td>
            <td>
                <partial name="_TableBtnDeletEditDeatails" model="item.Id" />

            </td>
        </tr>
        }
    </tbody>
</table>
